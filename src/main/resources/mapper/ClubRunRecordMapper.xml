<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.keep.mapper.CuldUserMapper">

    <insert id="saveUser" parameterType="com.keep.domain.ClubUser">
        <selectKey keyProperty="userId" order="AFTER" resultType="java.lang.Long">
            select LAST_INSERT_ID()
        </selectKey>
        INSERT INTO
            club_user (
            nick_name,
            title,
            ding_id,
            tenant_id,
            create_time )
        VALUES
            (#{nickName},
             #{title},
             #{dingId},
             #{telnetId},
             now())
    </insert>
</mapper>
